  "chapter": 26,
  "verses": [
    {
      "verse": 1,
      "text": "וַיַּ֥עַן אִיֹּ֗וב וַיֹּאמַֽר׃"
    },
    {
      "verse": 2,
      "text": "מֶה־עָזַ֥רְתָּ לְלֹא־כֹ֑חַ הֹ֝ושַׁ֗עְתָּ זְרֹ֣ועַ לֹא־עֹֽז׃"
    },
    {
      "verse": 3,
      "text": "מַה־יָּ֭עַצְתָּ לְלֹ֣א חָכְמָ֑ה וְ֝תוּשִׁיָּ֗ה לָרֹ֥ב הֹודָֽעְתָּ׃"
    },
    {
      "verse": 4,
      "text": "אֶת־מִ֭י הִגַּ֣דְתָּ מִלִּ֑ין וְנִשְׁמַת־מִ֝י יָצְאָ֥ה מִמֶּֽךָּ׃"
    },
    {
      "verse": 5,
      "text": "הָרְפָאִ֥ים יְחֹולָ֑לוּ מִתַּ֥חַת מַ֝֗יִם וְשֹׁכְנֵיהֶֽם׃"
    },
    {
      "verse": 6,
      "text": "עָרֹ֣ום שְׁאֹ֣ול נֶגְדֹּ֑ו וְאֵ֥ין כְּ֝ס֗וּת לָֽאֲבַדֹּֽון׃"
    },
    {
      "verse": 7,
      "text": "נֹטֶ֣ה צָפֹ֣ון עַל־תֹּ֑הוּ תֹּ֥לֶה אֶ֝֗רֶץ עַל־בְּלִי־מָֽה׃"
    },
    {
      "verse": 8,
      "text": "צֹרֵֽר־מַ֥יִם בְּעָבָ֑יו וְלֹא־נִבְקַ֖ע עָנָ֣ן תַּחְתָּֽם׃"
    },
    {
      "verse": 9,
      "text": "מְאַחֵ֥ז פְּנֵי־כִסֵּ֑ה פַּרְשֵׁ֖ז עָלָ֣יו עֲנָנֹֽו׃"
    },
    {
      "verse": 10,
      "text": "חֹֽק־חָ֭ג עַל־פְּנֵי־מָ֑יִם עַד־תַּכְלִ֖ית אֹ֣ור עִם־חֹֽשֶׁךְ׃"
    },
    {
      "verse": 11,
      "text": "עַמּוּדֵ֣י שָׁמַ֣יִם יְרֹופָ֑פוּ וְ֝יִתְמְה֗וּ מִגַּעֲרָתֹֽו׃"
    },
    {
      "verse": 12,
      "text": "בְּ֭כֹחֹו רָגַ֣ע הַיָּ֑ם *וּבִתוּבְנָתֹו (וּ֝בִתְבוּנָתֹ֗ו) מָ֣חַץ רָֽהַב׃"
    },
    {
      "verse": 13,
      "text": "בְּ֭רוּחֹו שָׁמַ֣יִם שִׁפְרָ֑ה חֹֽלֲלָ֥ה יָ֝דֹ֗ו נָחָ֥שׁ בָּרִֽיחַ׃"
    },
    {
      "verse": 14,
      "text": "הֶן־אֵ֤לֶּה ׀ קְצֹ֬ות *דַּרְכֹּו (דְּרָכָ֗יו) וּמַה־שֵּׁ֣מֶץ דָּ֭בָר נִשְׁמַע־בֹּ֑ו וְרַ֥עַם *גְּבוּרָתֹו (גְּ֝בוּרֹותָ֗יו) מִ֣י יִתְבֹּונָֽן׃ ס"
    }
  ]
